            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "frameworksBackend": {
            "region": "asia-east1"
        },
        "headers": [{
            "source": ".",
            "headers": [{
                "key": "Access-Control-Allow-Origin",
                "value": "*"
            }]
        }]
    }

}