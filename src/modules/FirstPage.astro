        document.addEventListener('DOMContentLoaded', function () {
        const list = document.getElementById('list-1');
        let isDragging = false;
        let startX = 0;
        let delta = -3000;
        let delX = 0;

        list?.addEventListener('mousedown', (e) => {
            isDragging = true;
            startX = e.clientX;
            delta = delX + delta;
            list.style.transition = 'none';
        });

        document.addEventListener('mousemove', (e) => {
            if (isDragging) {
                
                delX = e.clientX - startX;
                list.style.transform = `translateX(${delX + delta}px)`;
                const firstIcon = list.firstElementChild;
                const lastIcon = list.lastElementChild;
                const rect = list.getBoundingClientRect();
                const iconRect = firstIcon.getBoundingClientRect();
                console.log(iconRect.left);
                if (iconRect.left >0) {
                    delta = -3000 - delX;
                    
                }
            }
        });
        document.addEventListener('mouseup', () => {
            if (isDragging) {
                isDragging = false;
                

            }
            if (window.getSelection) {
            if (window.getSelection().empty) {  // Chrome
                window.getSelection().empty();
            } else if (window.getSelection().removeAllRanges) {  // Firefox
                window.getSelection().removeAllRanges();
            }
            } else if (document.selection) {  // IE
                document.selection.empty();
            }
        });

       
    });
    
        

       
