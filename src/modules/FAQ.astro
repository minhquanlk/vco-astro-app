<script>
    document.addEventListener('DOMContentLoaded', () => {
        var i = 0;
        const faqTitles = document.querySelectorAll('.faq-title');
        const faqContents = document.querySelectorAll('.faq-content');
        const videos = document.querySelectorAll('.video-faq');
        faqTitles.forEach((title) => {
            title.addEventListener('click', () => {
                const content = title.nextElementSibling;
                const isExpanded = content.classList.contains('expanded');

                // First, close all FAQ contents
                if (!isExpanded) {
                    faqContents.forEach((item, index) => {
                        if (item !== content) {
                            item.classList.remove('expanded');
                            item.style.maxHeight = null;
                        }
                        videos[index].contentWindow.postMessage('{"event":"command","func":"pauseVideo","args":""}', '*');

                        videos[index].classList.remove('video-visible');
                    });

                    // Close all FAQ titles
                    faqTitles.forEach((item) => {
                        if (item !== title) {
                            item.classList.remove('expanded');
                        }
                    });

                    // Then toggle the clicked one
                    title.classList.toggle('expanded', !isExpanded);
                    content.classList.toggle('expanded', !isExpanded);
                }
                // If expanding, set max-height to the scrollHeight value
                if (!isExpanded) {
                    content.style.maxHeight = content.scrollHeight + "px";
                    const videoID = title.getAttribute('data-video');
                    const video = document.querySelector(`.${videoID}`);

                    video.classList.add('video-visible');



                } else {
                    content.style.maxHeight = null;
                }
            });
            i++;
        });
    });
  
    

</script>
